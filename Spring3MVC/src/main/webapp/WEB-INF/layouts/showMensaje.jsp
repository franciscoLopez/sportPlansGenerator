<%@ include file="/WEB-INF/controllers/configuration/includes.jsp"%>
<c:if test="${!empty mensaje_ok}">
	<c:forEach items="${mensaje_ok}" var="mensaje">
		<div class="line _ok">${mensaje}</div>
	</c:forEach>
</c:if>

<c:if test="${!empty mensaje_error}">
	<c:forEach items="${mensaje_error}" var="mensaje">
		<div class="line _error">${mensaje}</div>
	</c:forEach>
</c:if>
<c:if test="${!empty mensaje_info}">
	<c:forEach items="${mensaje_info}" var="mensaje">
		<div class="line _info">${mensaje}</div>
	</c:forEach>
</c:if>