package com.project.enumerados;

import java.io.Serializable;

public enum TipoNotification implements Serializable{

	info,error;
	
	public String toString(){
		switch(this){
		case info:
			return "info";
		case error:
			return "error";
		}
		return null;
	}
	
	public Integer getNumOrden(){
		return this.ordinal(); 
	}	
}
